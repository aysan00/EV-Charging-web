<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>EV Charging Station Maps</title>
  <style>
    #map {
      height: 400px;
      width: 65%;
    }
  </style>
</head>
<body>
  <h1>Charging Stations</h1>
  <div id="map"></div>

  <script>
    function initMap() {
      var map = new google.maps.Map(document.getElementById('map'), {
        center: {lat: 10.7589, lng: 78.8132},
        zoom: 17
      });

      var marker = new google.maps.Marker({
        position: {lat: 10.7589, lng: 78.8132},
        map: map,
        title: 'Hello World!'
      });
    }
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAGFF4C-1scUH8_YwkNJdD3ydcsM8AzHqg&callback=initMap" async defer></script>
</body>
</html>
